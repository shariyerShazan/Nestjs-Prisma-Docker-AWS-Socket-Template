{
  "name": "nestjs-prisma-aws-starter",
  "version": "0.0.1",
  "description": "This repository contains the source code for the NestJS + Prisma + AWS Starter.",
  "author": "https://github.com/shahadathhs",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "build": "npx nest build",
    "start": "npx prisma generate && npx prisma migrate deploy && node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .prettierignore --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "ci:check": "pnpm format && pnpm lint && pnpm db:validate && pnpm build && pnpm start",
    "ci:fix": "pnpm format:fix && pnpm lint:fix",
    "prisma": "npx prisma",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "db:generate": "npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:validate": "npx prisma validate",
    "db:format": "npx prisma format",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/client-runtime-utils": "^7.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.1",
    "bullmq": "^5.65.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "he": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "mime-types": "^3.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.13",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/swagger": "^11.2.3",
    "@types/bcrypt": "^6.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.5",
    "@types/he": "^1.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.1",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.6",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "node-emoji": "^2.2.0",
    "prettier": "^3.7.1",
    "prisma": "^7.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "yocto-spinner": "^1.0.0"
  }
}